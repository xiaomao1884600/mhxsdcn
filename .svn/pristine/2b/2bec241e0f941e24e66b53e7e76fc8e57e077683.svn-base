/* edge */
.edge{width:100%;background:url("../img/bg.jpg") repeat;color:#272727;margin:0 auto;}
.warp{padding:0 10px;}
.top{position:relative;}
.top .form_tel{position:absolute;top:0;top:71%;height:30px;width:100%;}
.top .form_tel p{width:82%;height:30px;margin:0 auto;clear:both;}
.top .form_tel p a{display:block;width:100%;height:22px;background:none;float:right;}
.top img  {max-width:100% !important;
	mywidth:expression(onload=function() {
	this.style.width=(this.offsetWidth >document.getElementById("pic").scrollWidth*8/10)? "100%": "auto";
	}
)}
html h1{font-size:1.5em;text-align:center;color:#d10b0b;padding:10px 0 0 ;}

.tel{clear:both;}
.tel a{display:block;padding:10px;text-align:center;}
.tel a.btn b{display:inline-block;background:#d10b03;border-radius:3px;-moz-border-radius:3px;-webkit-border-radius:3px;padding:0px 15px;margin:0 auto;text-align:center;color:#fff;font-weight:500;font-size:16px;
}
.tel span{background:url("../img/tel.jpg") no-repeat 0 14px;display:block;padding:10px 0 10px 30px;width:220px;height:20px;margin:0 auto;font-size:1.2em;}
.tel span a{color:#333;float:left;padding:0 5px;}

.tel2 a.btn b{font-size:18px;font-weight:500;width:auto;display:inline-block;padding:3px 10px;}

.tq{border-bottom:1px solid #ccc;margin:0 10px;}
.tq h2{font-weight:500;font-size:1.5em;line-height:1.3em;padding:10px 0 0;}
.tq h2 span{font-weight:900;}
.tq ul{clear:both;overflow:hidden;text-align:center;}
.tq ul li{padding:10px 0;}
.tq ul li a{background:#febf5c;display:block;margin:0 auto;width:76px;height:76px;text-align:center;line-height:76px;border-radius:50px;-moz-border-radius:50px;-webkit-border-radius:50px;}
.tq ul li p{color:#727272;}
.tq ul li b{font-size:14px;}
.tit{text-align:left;color:#202020;font-size:18px;padding:10px 10px 0;line-height:1.6em;}
.tit span{color:#da311f;}
.tit p{color:#727272;font-weight:500;font-size:12px;line-height:20px;}
.tit b{font-size:16px;font-weight:900;}
.align_l{text-align:left;}
.linegree{border-bottom:1px solid #ccc;padding-bottom:10px;}
.pad10{padding:10px;}
.fc{clear:both;}
.fc li{text-align:center;padding:10px 0;}
.fc li img{border:1px solid #ccc;}
.fc h4{color:#f87500;font-size:16px;font-weight:500;padding:5px 0 0;}
.fc p{font-size:12px;color:#727272;padding:0 15%;}

.xy{clear:both;padding:0 0 10px 0;}
.xy ul{clear:both;}
.xy ul li{width:48%;float:left;margin:2% 1%;overflow:hidden;text-align:center;}
.xy ul li img{max-width:90%;box-shadow:1px 1px 5px 1px #ccc;
	mywidth:expression(onload=function() {
	this.style.width=(this.offsetWidth >document.getElementById("pic").scrollWidth*8/10)? "90%": "auto";
	}
)}

.cont{padding:0 10px;margin-top:0;}
.cont .tel span{display:block;cleaR:both;}
.cont p{font-size:12px;}
.zg{bordeR:3px solid #ff7e00;padding:5px;margin:0 0 10px;}

.align_c{text-align:center;padding:0 10px;}
.c{text-align:center;}
p.news{font-size:12px;padding:0 10px;}
h2.b_g{background:#221a80;color:#fff;font-size:14px;font-weight:500;text-align:Center;padding:2px 0;margin:15px 0;}
